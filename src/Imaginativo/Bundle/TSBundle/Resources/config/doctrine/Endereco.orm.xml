<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Imaginativo\Bundle\TSBundle\Entity\Endereco">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="logradouro" type="string" column="logradouro" length="255"/>
    <field name="bairro" type="string" column="bairro" length="255"/>
    <field name="cidade" type="string" column="cidade" length="255"/>
    <field name="uF" type="string" column="UF" length="2"/>
    <field name="cEP" type="string" column="CEP" length="8"/>
  </entity>
</doctrine-mapping>
