{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Endereco</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Logradouro</th>
                <td>{{ entity.logradouro }}</td>
            </tr>
            <tr>
                <th>Bairro</th>
                <td>{{ entity.bairro }}</td>
            </tr>
            <tr>
                <th>Cidade</th>
                <td>{{ entity.cidade }}</td>
            </tr>
            <tr>
                <th>Uf</th>
                <td>{{ entity.uF }}</td>
            </tr>
            <tr>
                <th>Cep</th>
                <td>{{ entity.cEP }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('endereco') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('endereco_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
